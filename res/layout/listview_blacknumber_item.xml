<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_margin="5dp"
    android:layout_height="match_parent" >
    <TextView 
        android:id="@+id/tv_black_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="19sp"
        android:text="13888888888"
        />
    
     <TextView 
        android:id="@+id/tv_black_Contas"
        android:layout_below="@id/tv_black_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#8d8d8d"
        android:text="拦截类型"
        />
     
     <ImageButton
         android:id="@+id/ibttn_deleblackNum"
         android:layout_alignParentRight="true" 
         android:background="@drawable/black_number_delete_bj"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         />

</RelativeLayout>
